import sendgrid
import os
from sendgrid.helpers.mail import *

sg = sendgrid.SendGridAPIClient(apikey='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')
from_email = Email("test@example.com")
subject = "Hello World from the SendGrid Python Library!"
#to_email = [Email("easy.goal87@yahoo.ie"), Email("prisco.napoli@gmail.com")]
to_email = [Email("easy.goal87@yahoo.ie")]
content = Content("text/plain", "Hello, Email!")
mail = Mail(from_email, subject, to_email, content)
response = sg.client.mail.send.post(request_body=mail.get())
print(response.status_code)
print(response.body)
print(response.headers)